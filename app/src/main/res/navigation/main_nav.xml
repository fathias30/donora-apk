<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.donora_ui.ui.fragment.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_stoockFragment"
            app:destination="@id/stoockFragment" />
    </fragment>
    <fragment
        android:id="@+id/stoockFragment"
        android:name="com.example.donora_ui.ui.fragment.StoockFragment"
        android:label="fragment_stoock"
        tools:layout="@layout/fragment_stoock" >
        <action
            android:id="@+id/action_stoockFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
</navigation>